.container {
  height 100%
  width 100%
  overflow auto

  .header {
    padding-left 10px
    margin-left 20px
    margin-top 20px
    border-left 4px solid #009688
    font-size 16px
    font-weight 300
    color #212121
  }

  .search {
    display block
    width 200px !important
    margin 20px 15px
  }

  .table {
    width 100%
    padding 0 15px
    overflow hidden
  }
}